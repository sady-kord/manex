<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView
        android:id="@+id/text_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/tabbedlist_title_marginstart"
        android:layout_marginBottom="@dimen/tabbedlist_title_marginbottom"
        android:layout_marginTop="@dimen/tabbedlist_title_margintop"

        android:textSize="@dimen/tabbedlist_title_textsize"
        android:textColor="@color/tabbedlist_title_textcolor"
        android:fontFamily="@font/iranyekan_bold"

        tools:text="عنوان"
        />

</FrameLayout>